# Name for the network to deploy to ("hardhat-remote", "rinkeby", "local")
CONTRACTS_TARGET_NETWORK=rinkeby

# Private key that will send deployment transactions
CONTRACTS_DEPLOYER_KEY=

# RPC URL connected to the L1 chain we're deploying to
CONTRACTS_RPC_URL=https://rinkeby.rpc.tokamak.network

# Your Etherscan API key for the L1 network
ETHERSCAN_API_KEY=

AUTOMATICALLY_TRANSFER_OWNERSHIP=false
VERIFY_CONTRACTS=true
